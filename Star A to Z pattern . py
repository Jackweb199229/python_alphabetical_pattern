for i in range (7):
	for j in range(5):
		if(( (i!=0) and (j==0 or j==4)) or((i==0 or i== 3) and (j>0and j<4))):
			print ("*" , end=" ")
		else:
			print(end="  ")
	print(end=" ")
	for j in range(5):
		if (j==0 or((i==0 and j!=4) or (i==3 and j!=4 )or (i==6 and j!=4))) or (j==4 and (i>0and i<3) )or (j==4 and (i>3 and i<6)):
			print ("*" , end=" ")
		else:
			print(end="  ")
	print(end=" ")
	for j in range(5):
 			if (j==0 and i!=0 and i!=6) or ((i==0 or i==6) and (j>0)):
 				print("*" , end=" ")
 			else :
 				print(end="  ")
	print(end="  ")
	for j in range(5):
		if (j==0) or ((j==4) and (i!=0 and  i!=6)) or ((i==0 or i==6) and j<=3):
			print("*" , end=" ")
		else:
			print(end="  ")
	print()
print("\n")
# Pattern E
for i in range(7):
	for j in range(5):
		if (j==0) or ((j>0 and j<=4) and (i==0 or i==3 or i==6)):
			print("*" , end=" ")
		else:
			print(end="  ")
	print(end="    ")
#Pattern F
	for j in range(5):
		if (j==0) or ((j>0 and j<=4) and (i==0 or i==3 )):
			print("*" , end=" ")
		else:
			print(end="  ")
	print(end="     ")
#Pattern G
	for j in range(5):
		    	if (j==0 and i!=0 and i!=6) or ((i ==0 or i==6) and (j>0)) or ((j==4) and (i>=3 and i<=6)) or (i==3 and j>1):
		    		print("*" , end=" ")
		    	else :
		    		print(end="  ")
	print()
print("\n")
# Pattern H
for i in range(7):
 		for j in range(5):
 			if (j==0 or j==4) or (i==3 and (j>0 and j<4)):
 				print("*" , end=" ")
 			else :
 				print(end="  ")
 		print(end="  ")
#Pattern I
 		for j in range(5):
 			if (i==0 or i==6) or j==2:
 				print("*" , end=" ")
 			else :
 				print(end="  ")
 		print(end="  ")
#Pattern J
 		for j in range(5):
 			if  (i==0 or j==2) or (j==0) and (i>3 and i<6) or (i==6 and j==1):
 				print("*",end=" ")
 			else:
 				print(end="  ")
 		print()
print("\n") 
#Pattern k		
for i in range(7):
	for j in range(5):
		if  (j==0 or j==4-i or j==i-2):
			print("*",end=" ")
		else:
			print(end="  ")
	print(end="  ")
	for j in range(5):
		if  (j==0 or i==6):
			print("*",end=" ")
		else:
			print(end="  ")
	print(end="  ")
	for j in range(7):
		if  (j==0 or j==6)or (i==j and j<=3) or(i==1 and j==5) or (i==2 and j==4):
			print("*",end=" ")
		else:
			print(end="  ")
	print()